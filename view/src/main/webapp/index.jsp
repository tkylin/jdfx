<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="copyright" content="All Rights Reserved, Copyright (C) 2017, Tkylin, Ltd." />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=no"/>
    <title>登录</title>
    <style type="text/css">
        body{font:normal normal normal 13px/166.6% helvetica,arial,sans-serif;-webkit-text-size-adjust:none;background-color:#D3EAF7;}
        body,ul,ol,p{margin:0;padding:0}
        em{font-style:normal;}
        a{color:#005590}
        .page{background:#D3EAF7 url(data:image/png;base64,) repeat-x;}
        .page{padding-bottom:60px}
        .mod-logo{text-align:center;height:87px;}
        .mod-logo img{display:block;margin:auto;}
        .mod-error{border:1px solid #d90000;color:#d90000;background:#ffd3d3;width:270px;margin:12px auto;padding:8px;line-height:1.6;display:none}

        .mod-login{min-height:150px;width:288px;margin:auto;}
        .mod-login .iptBox{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;-ms-box-sizing:border-box;border:1px solid #b5b5b5;border-radius:5px;background:#fff -webkit-gradient(linear,0 0,0 100%,from(#fff),to(#f6f6f6));-webkit-box-shadow:0 2px 3px #A6C1D2;-moz-box-shadow:0 2px 3px #A6C1D2;-ms-box-shadow:0 2px 3px #A6C1D2;position:relative}
        .mod-login .iptBox .item{border-bottom:1px solid #e2e2e2;padding:0 14px;height:50px;line-height:50px;}
        .mod-login .iptBox .item:last-child{border:0}
        .mod-login .iptBox .lb{display:inline-block;font-size:14px;width:60px}
        .mod-login .iptBox input,
        .mod-login .iptBox select{font-size:14px;width:190px;height:30px;border:0;background:transparent;-webkit-appearance:none;padding:0}
        .mod-login .iptBox input{font-size:18px;}
        .mod-login .iptBox .item-select{background:url(data:image/gif;base64,R0lGODlhDQAIAMQAAOTk5OLi4uDg4MrKysnJycjIyMfHx8TExMPDw729vby8vLCwsKmpqaSkpKGhoZubm4mJiYiIiP///wAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAACH5BAEHABIALAAAAAANAAgAAAUtoFQoUmlKySAJELSc0tICEtI2JhNFhnm0j4CjRYDZWi0VTPJrFZYmmxJaEixDADs=) 95% 50% no-repeat}
        /*#username{width:115px}*/
        .mod-login .iptBox .domain{color:#999;font-size:14px;position:absolute;top:0;right:8px}
        .mod-login .submit{text-align:center;margin:20px}
        .mod-login .loginBtn{border:0;padding:0;background:0;width:128px;height:36px;font-size:16px;-webkit-border-image:url(data:image/png;base64,) 0 4 0 4 stretch stretch;-moz-border-image:url(data:image/png;base64,) 0 4 0 4 stretch stretch;-ms-border-image:url(data:image/png;base64,) 0 4 0 4 stretch stretch;border-width:0 4px;font-weight:bold;text-shadow:0 -1px 2px #000;color:#fff}
        .mod-login .reg{text-align:center}

        .mod-footer{text-align:center}
        .mod-footer p{margin:5px 0}
        .mod-footer strong,
        .mod-footer a,
        .mod-footer .copy{margin:0 5px}
        .mod-footer .copyright{color:#999}

    </style>
</head>
<body>
<div class="page">
    <div class="mod mod-logo">
        <%--<img src="data:image/gif;base64," alt="网易163免费邮">--%>
    </div>

    <div class="mod mod-error" id="errMsg"></div>

    <div class="mod mod-login">
        <form id="loginForm" method="post" >
            <div class="iptBox">
                <div class="item">
                    <label class="lb">用户名：</label>
                    <input type="text" name="username" id="username" maxlength="50">
                    <%--<em class="domain">@163.com</em>--%>
                </div>
                <div class="item">
                    <label class="lb">密　码：</label>
                    <input type="password" name="password" id="password">
                </div>
                <div class="item item-select">
                    <label class="lb">记住我</label>
                    <select id="chkAutoLogin" name="savelogin">
                        <option value="1">是</option>
                        <option value="0">否</option>
                    </select>
                </div>
            </div>

            <div class="submit">
                <button type="submit" class="loginBtn">登 录</button>
            </div>

            <div class="reg">
                <%--<a href="https://ssl.mail.163.com/e.mail.163.com/mobilemail/wap/reg.do" target="_blank">马上注册</a> |
                <a href="http://reg.163.com/getpasswd/RetakePassword.jsp" target="_blank">忘记密码</a>--%>
            </div>
        </form>
    </div>

    <div class="mod mod-footer">

    </div>
</div>
</body>
</html>
